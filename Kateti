#include <iostream>;
using namespace std;
int main ();
{
    float a, b, c, p;
    cout<<"a=";
    cin >>a;
    cout<<"b=";
    c=sqrt(pow(a, 2) + pow(b, 2))
    p=a+b+c
    cout << "Гипотенуза треугольника равна" << c << endl;
    cout << "Периметр треугольника равен" << p << end1;
    return 0;
}
